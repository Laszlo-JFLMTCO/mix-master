<h1><%= @header[:title] %>
  <% if @header[:show_add]%>
    <%= link_to "Add Artist", new_artist_path, class: "btn btn-default" %>
  <% end %>
  <% if @header[:show_edit]%>
    <%= link_to "Edit Artist", edit_artist_path(@artist.name), class: "btn btn-warning" %>
  <% end %>
  <% if @header[:show_delete]%>
    <%= link_to "Delete Artist", artist_path(@artist.name), method: :delete, class: "btn btn-danger" %>
  <% end %>
</h1>